{
  "name": "react-jwt-auth",
  "version": "0.5.2",
  "description": "JSON Web Token authentication and authorization component for React",
  "repository": {
    "type": "git",
    "url": "https://github.com/fullstackforger/react-jwt-auth"
  },
  "files": [
    "lib",
    "README"
  ],
  "main": "lib/cjs/index",
  "jsnext:main": "lib/es6/index",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./internals/test.setup.js ./src/**/*.spec.js",
    "build": "npm run build-cjs && npm run build-es",
    "build-cjs": "rimraf lib && cross-env set BABEL_ENV=cjs && babel ./src -d lib/cjs --ignore **/*.spec.js",
    "build-es": "rimraf es6 && cross-env BABEL_ENV=es babel ./src -d lib/es6 --ignore **/*.spec.js"
  },
  "keywords": [
    "react",
    "auth",
    "jwt",
    "authentication",
    "token",
    "component"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.2",
    "fetch-mock": "^5.5.0",
    "jsdom": "^9.6.0",
    "mocha": "^3.1.2",
    "mocha-loader": "^1.0.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-router": "^2.8.1",
    "rimraf": "^2.5.4"
  }
}
